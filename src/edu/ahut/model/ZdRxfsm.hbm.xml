<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="edu.ahut.model.ZdRxfsm" table="ZD_RXFSM" schema="PGDB">
        <id name="id" type="long">
            <column name="ID" precision="10" scale="0" />
            <generator class="native" />
        </id>
        <property name="rxfsm" type="string">
            <column name="RXFSM" length="10" not-null="true" unique="true" />
        </property>
        <property name="rxfs" type="string">
            <column name="RXFS" length="20" />
        </property>
        <property name="cs" type="long">
            <column name="CS" precision="10" scale="0" />
        </property>
        <property name="scm" type="string">
            <column name="SCM" length="10" />
        </property>
        <property name="sfyz" type="string">
            <column name="SFYZ" length="1" />
        </property>
        <property name="pxh" type="long">
            <column name="PXH" precision="10" scale="0" />
        </property>
        <property name="sfyx" type="string">
            <column name="SFYX" length="1" />
        </property>
        <property name="bz" type="string">
            <column name="BZ" length="200" />
        </property>
    </class>
</hibernate-mapping>
