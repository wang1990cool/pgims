        </id>
        <property name="hyzkm" type="string">
            <column name="HYZKM" length="20" not-null="true" unique="true" />
        </property>
        <property name="hyzk" type="string">
            <column name="HYZK" length="20" />
        </property>
        <property name="pxh" type="double">
            <column name="PXH" precision="0" />
        </property>
        <property name="sfyx" type="string">
            <column name="SFYX" length="1" />
        </property>
        <property name="bz" type="string">
            <column name="BZ" length="200" />
        </property>
    </class>
</hibernate-mapping>
