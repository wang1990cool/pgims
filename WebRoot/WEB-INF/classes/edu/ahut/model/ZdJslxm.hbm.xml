<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="edu.ahut.model.ZdJslxm" table="ZD_JSLXM" schema="PGDB">
        <id name="id" type="double">
            <column name="ID" precision="0" />
            <generator class="native" />
        </id>
        <property name="jslxm" type="string">
            <column name="JSLXM" length="10" not-null="true" unique="true" />
        </property>
        <property name="jslx" type="string">
            <column name="JSLX" length="20" />
        </property>
        <property name="pxh" type="long">
            <column name="PXH" precision="10" scale="0" />
        </property>
        <property name="sfyx" type="string">
            <column name="SFYX" length="1" />
        </property>
        <property name="bz" type="string">
            <column name="BZ" length="200" />
        </property>
    </class>
</hibernate-mapping>
